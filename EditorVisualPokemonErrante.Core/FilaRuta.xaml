<UserControl x:Class="EditorVisualPokemonErrante.Core.FilaRuta"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EditorVisualPokemonErrante"
             mc:Ignorable="d" d:DesignWidth="300" Height="33.144">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="txtRuta" Background="LightBlue"/>
        <TextBox Grid.Column="1" x:Name="txtRebote1" Background="LightGreen"/>
        <TextBox Grid.Column="2" x:Name="txtRebote2" Background="LightGreen"/>
        <TextBox Grid.Column="3" x:Name="txtRebote3" Background="LightGreen"/>
        <TextBox Grid.Column="4" x:Name="txtRebote4" Background="LightGreen"/>
        <TextBox Grid.Column="5" x:Name="txtRebote5" Background="LightGreen"/>
        <TextBox Grid.Column="6" x:Name="txtRebote6" Background="LightGreen"/>
    </Grid>
</UserControl>
