<Window x:Class="EditorVisualPokemonErrante.PrevisualizarScriptXSE"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EditorVisualPokemonErrante"
        mc:Ignorable="d"
        Title="Preview script XSE" Height="300" Width="300">
    <StackPanel>
        <Image x:Name="imgVersion" MaxHeight="150"/>
        <TextBox x:Name="txtScript" IsReadOnly="True"/>
    </StackPanel>
</Window>
